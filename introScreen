//replace corresponding forever function
forever(function () {
    if (startPressed) {
        camx = mySprite4.x + 50
        scene.centerCameraAt(camx, mySprite4.y)
        mySprite2.x = mySprite4.x + 120
        if (mySprite4.isHittingTile(CollisionDirection.Bottom) && jumped == 1) {
            jumped = 0
        }
    }
})

